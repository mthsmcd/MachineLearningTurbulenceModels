{
    version         2;
    format          ascii;
    class           dictionary;
    location        "system";
    object          fvOptions;
}

momentumSource
{
    type            meanVelocityForce;
    active          yes;
    meanVelocityForceCoeffs
    {
        selectionMode   all;
        fields          ( U );
        Ubar            ( 0.020188 0 0 );
    }
}


